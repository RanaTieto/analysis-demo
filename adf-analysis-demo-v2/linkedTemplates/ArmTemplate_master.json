{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "adf-analysis-demo-v2"
		},
		"AzureBlobStorage_conn_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage_conn'"
		},
		"AzureSqlDatabase1_conn_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1_conn'"
		},
		"Conn_AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Conn_AzureBlobStorage'"
		},
		"address_test_conn_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'address_test_conn'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_conn_connectionString": {
						"value": "[parameters('AzureBlobStorage_conn_connectionString')]"
					},
					"AzureSqlDatabase1_conn_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_conn_connectionString')]"
					},
					"Conn_AzureBlobStorage_connectionString": {
						"value": "[parameters('Conn_AzureBlobStorage_connectionString')]"
					},
					"address_test_conn_connectionString": {
						"value": "[parameters('address_test_conn_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}